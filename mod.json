{
    "geode": "5.0.0-alpha.1",
    "gd": {
        "win": "2.2081",
        "android": "2.2081",
        "mac": "2.2081",
        "ios": "2.2081"
    },
    "id": "timestepyt.deltarune_textboxes",
    "name": "Deltarune Textboxes",
    "version": "v1.6.0-alpha.1",
    "developer": "TimeStepYT",
    "description": "Makes Popups look like Deltarune",
    "links": {
        "source": "https://github.com/TimeStepYT/DeltaruneTextboxes"
    },
    "tags": [
        "customization"
    ],
    "dependencies": {
        "geode.node-ids": ">=1.19.0",
        "geode.custom-keybinds": {
            "importance": "required",
            "version": ">=v1.7.1",
            "platforms": [
                "mac",
                "android64"
            ]
        },
        "alphalaneous.alphas-ui-pack": ">=v1.0.1-beta.1"
    },
    "resources": {
        "sprites": [
            "resources/sprites/*.png"
        ],
        "fonts": {
            "Determination": {
                "size": 78,
                "path": "resources/fonts/determination-extended.ttf",
                "charset": "32-126,8226",
                "outline": 0
            },
            "ComicSans": {
                "size": 78,
                "path": "resources/fonts/comic-sans-pixel.ttf",
                "charset": "32-126,8226",
                "outline": 0
            },
            "Papyrus": {
                "size": 78,
                "path": "resources/fonts/papyrus-pixel.ttf",
                "charset": "32-126,8226",
                "outline": 0
            }
        },
        "files": [
            "resources/sounds/SND_TXT1.wav",
            "resources/sounds/SND_TXT2.wav",
            "resources/sounds/snd_txtal.wav",
            "resources/sounds/snd_txtasg.wav",
            "resources/sounds/snd_txtasr.wav",
            "resources/sounds/snd_txtsans.wav",
            "resources/sounds/snd_txtpap.wav",
            "resources/sounds/snd_txtund.wav",
            "resources/sounds/snd_txttor.wav",
            "resources/sounds/snd_txtsus.wav",
            "resources/sounds/snd_txtral.wav",
            "resources/sounds/snd_txtlan.wav",
            "resources/sounds/snd_txtnoe.wav",
            "resources/sounds/snd_txtber.wav",
            "resources/sounds/snd_txtjok.wav",
            "resources/sounds/snd_txtspam.wav",
            "resources/sounds/snd_txtspam2.wav",
            "resources/sounds/snd_txtq.wav",
            "resources/sounds/snd_txtger.wav",
            "resources/sounds/snd_txtcar.wav",
            "resources/sounds/snd_txtjack.wav",
            "resources/sounds/snd_txtten1.wav",
            "resources/sounds/snd_txtten2.wav",
            "resources/sounds/snd_txtten3.wav",
            "resources/sounds/snd_txtten4.wav",
            "resources/sounds/snd_txtten5.wav",
            "resources/sounds/snd_txtten6.wav",
            "resources/sounds/snd_txtten7.wav",
            "resources/sounds/snd_txtten8.wav",
            "resources/sounds/snd_txtten9.wav",
            "resources/sounds/snd_txtten10.wav",
            "resources/shaders/*",
            "resources/g.jpg"
        ]
    },
    "settings": {
        "dontRestrictWidth": {
            "name": "Don't limit width",
            "type": "bool",
            "default": false,
            "description": "Doesn't limit the <cy>textbox width</c> to max width for <cj>16:9</c>"
        },
        "disableClickToProgress": {
            "name": "Don't progress by clicking",
            "type": "bool",
            "default": false,
            "description": "Disables <cy>clicking</c> to progress textboxes",
            "platforms": [
                "win",
                "mac"
            ]
        },
        "coloredPortraits": {
            "name": "Colored Portraits",
            "type": "bool",
            "default": false,
            "description": "<cr>Disables</c> the <cy>grayscale</c> effect on the <cj>dialog portraits</c>."
        },
        "noGradientOverlay": {
            "name": "Disable gradient",
            "type": "bool",
            "default": false,
            "description": "<cr>Disables</c> the <cy>gradient</c> that you can see on <cj>colored text</c>"
        },
        "noShadow": {
            "name": "Disable shadow",
            "description": "<cr>Disables</c> the <cp>shadow</c> <cy>behind the text</c>",
            "type": "bool",
            "default": false
        },
        "noAnimation": {
            "name": "No Animation",
            "description": "<cr>Disables</c> the <cj>animated background</c>. <cy>(the colors of the corners change)</c>",
            "type": "bool",
            "default": false
        },
        "undertaleBG": {
            "name": "Undertale Background",
            "description": "<cj>Changes the background</c> of the textbox to the <cr>Undertale</c> one",
            "type": "bool",
            "default": false
        },
        "textRollingPause": {
            "name": "Text Rolling Pause",
            "type": "float",
            "min": 0,
            "max": 5,
            "default": 0.8,
            "control": {
                "arrow-step": 0.1,
                "big-arrow-step": 1
            },
            "description": "How many <cy>30th</c>s of a second it should <cj>pause</c> between characters for the <cg>rolling effect</c>"
        },
        "textSound": {
            "name": "Text Sound",
            "type": "string",
            "default": "Default",
            "description": "The <cy>sound</c> that plays for <cj>every letter</c> when rolling out",
            "one-of": [
                "Default",
                "Typewriter",
                "Toriel",
                "Sans",
                "Papyrus",
                "Undyne",
                "Alphys",
                "Asgore",
                "Asriel",
                "Susie",
                "Ralsei",
                "Lancer",
                "Jevil",
                "Noelle",
                "Berdly",
                "Queen",
                "Spamton",
                "Spamton NEO",
                "Tenna",
                "Carol",
                "Gerson",
                "Jackenstein",
                "No Sound"
            ]
        }
    }
}